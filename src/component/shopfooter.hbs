<div class="px-3">
    <section class="col-12">
        {{#each footer}}
        <div class="nav">
            {{#each this}}
            <div class="nav-item col-3">
                <a class="nav-link {{#if @last}} disabled{{/if}}" href="{{this.link}}">{{this.text}}</a>
            </div>
            {{/each}}
        </div>
        {{/each}}
    </section>
</div>