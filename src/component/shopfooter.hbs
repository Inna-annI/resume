<section class="container">
    <div class="row">
        <div class="col-12">
            {{#each footer}}
            <div class="nav col">
                <div class="m-3">
                    {{#each this}}
                    <div>
                        <a class="nav-link {{#if @last}}disabled{{/if}}" href="{{this.link}}">{{this.text}}</a>
                    </div>
                    {{/each}}
                </div>
                {{/each}}
            </div>
        </div>
    </div>
</section>